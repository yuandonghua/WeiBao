<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yingshiyuan.starpark">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.LoginActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".activity.WritePhoneActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".activity.WriteIdentifyingCodeActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".activity.SetNameWithPasswordActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".activity.SetEmailActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".activity.SetPasswordActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".activity.ContactInfoActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".activity.ManageAddressActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".activity.AddAddressActivity"
            android:theme="@style/BaseTheme" />

    </application>

</manifest>